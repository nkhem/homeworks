<!DOCTYPE html>
<html>
<head>
  <title>MusicApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if logged_in? %>
    <%= button_to "log out", session_url, method: :delete %>
    <%= button_to "BANDSSS", bands_url, method: :get %>
  <% end %>

<%= render "shared/errors" %>
<%= yield %>

</body>
</html>
